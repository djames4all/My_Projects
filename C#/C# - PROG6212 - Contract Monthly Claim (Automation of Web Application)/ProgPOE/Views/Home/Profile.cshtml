@model CMCS.Controllers.User

@{
    ViewData["Title"] = "Profile";
}
<!-- ST10393280 -->
<div class="container">
    <h1 class="mt-4">Profile</h1>

    <div class="card mt-4">
        <div class="card-body">
            <h3>@Model.Username</h3>
            <p><strong>Email:</strong> @Model.Email</p>
            <p><strong>Role:</strong> @Model.Role</p>
        </div>
    </div>
</div>
